03A5 12040F     182            lcall Send_SPI ; Read the next byte from the SPI Flash...
03A8 F580       183            mov P0, a ; WARNING: Remove this if not using an external DAC to use the pins of P0 as GPIO
03AA 2480       184            add a, #0x80
