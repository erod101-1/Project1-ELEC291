0358            122            
0358            123            ; Configure timer 1
0358 53890F     124            anl     TMOD, #0x0F ; Clear the bits of timer 1 in TMOD
035B 438910     125            orl     TMOD, #0x10 ; Set timer 1 in 16-bit timer mode.  Don't change the bits of timer 0
